@model List<trabajopalacasa.Models.ViewModels.SoliViewModels>
@{ 
    ViewBag.Title = "ListaSolicitud";
    Layout = "~/Views/Shared/_Layout_Solicitante.cshtml";
}

<h2>Solicitudes</h2>

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <thead class="table-dark">
                <tr>
                    <th>Titulo</th>
                    <th>Tipo Solicitud</th>
                    <th>Estado</th>
                    <th>Fecha</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var oElemento in Model)
                {
                    <tr>
                        <td>@oElemento.Titulo</td>
                        <td>@oElemento.Tipo_solicitud</td>
                        <td>@oElemento.Estado</td>
                        <td>@oElemento.Fecha</td> 
                    </tr>
}
            </tbody>
        </table>
    </div>
</div>

